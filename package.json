    "test:karma": "npm run test:unit",
    "eslint": "eslint \"./**/*.js\"",
    "lint": "npm run eslint",
    "start": "node server.js",
    "serve": "lite-server",
    "todo": "./node_modules/.bin/leasot \"./src/**/*.js\" --ignore \"./src/lib/**/*.js\" --tags error,info,fixme,review,bug,note,feature || true"
